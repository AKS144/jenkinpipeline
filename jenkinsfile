pipeline {
    agent any

    stages {
        stage('Hi') {
            steps {
                echo 'Hi World'
            }
        }
        stage('BUild') {
            steps {
                echo 'BUilding'
            }
        }
        stage('Deploy') {
            steps {
                echo 'Deploying'
            }
        }
        stage('Release') {
            steps {
                echo 'Releasing'
            }
        }
    }
}
